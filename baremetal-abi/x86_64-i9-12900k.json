{
  "llvm-target": "x86_64-unknown-none",
  "data-layout": "e-m:e-p270:32:32-p271:32:32-p272:64:64-i64:64-i128:128-f80:128-n8:16:32:64-S128",
  "arch": "x86_64",
  "target-endian": "little",
  "target-pointer-width": 64,
  "target-c-int-width": 32,
  "os": "none",
  "linker-flavor": "ld.lld",
  "linker": "rust-lld",
  "executables": true,
  "features": "+mmx,+sse,+sse2,+sse3,+ssse3,+sse4.1,+sse4.2,+avx,+avx2,+aes,+pclmulqdq,+rdrand,+rdseed,+bmi1,+bmi2,+lzcnt,+popcnt,+xsave,+xsaveopt,+xsavec,+xsaves,+movbe,+fma,-avx512f,-avx512cd,-avx512bw,-avx512dq,-avx512vl",
  "disable-redzone": true,
  "panic-strategy": "abort",
  "code-model": "kernel",
  "relocation-model": "static",
  "target-family": ["unix"],
  "metadata": {
    "description": "Intel i9-12900K Alder Lake bare-metal target (AVX-512 disabled for E-core compatibility)"
  }
}
